    <tbody>
        <tr style="height:81px;" id="kongbai"></tr>
        @foreach (var entity in Model)
        {
            OtherTiChengSum += entity.OtherTiCheng;
            shicaosum += entity.ShiCao_zhiding;
            xiangmushu += entity.KeShu_zhiding;

            _Yeji_Chuzhika +=   entity.Yeji_Chuzhika;
        
        
_Yeji_Liaochengka +=   entity.Yeji_Liaochengka;
        
        
_Yeji_Xiangmu +=   entity.Yeji_Xiangmu;
        
        
_Yeji_Chanpin +=   entity.Yeji_Chanpin;
        
        
_Yeji_TeshuChanpin+=   entity.Yeji_TeshuChanpin;
        
        
_Yeji_Teshuxiangmu+=   entity.Yeji_Teshuxiangmu;
        
        
_Yeji_Daxiangmu +=   entity.Yeji_Daxiangmu;
        
        
 _Yeji_Chuzhikasum +=  (entity.Yeji_Chuzhika + entity.Yeji_Liaochengka + entity.Yeji_Chanpin + entity.Yeji_TeshuChanpin + entity.Yeji_Xiangmu + entity.Yeji_Daxiangmu + entity.Yeji_Teshuxiangmu);
        
        
_KakouYeji_Liaocheng +=  (entity.KakouYeji_Liaocheng);
        
        
_KakouYeji_Xiangmu +=   entity.KakouYeji_Xiangmu;
        
        
_KakouYeji_Chanpin +=   entity.KakouYeji_Chanpin;
        
        
_KakouYeji_TeshuChanpin +=   entity.KakouYeji_TeshuChanpin;
        
_KakouYeji_Teshuxiangmu +=   entity.KakouYeji_Teshuxiangmu;
        
_KakouYeji_Daxiangmu +=   entity.KakouYeji_Daxiangmu;
        
_KakouYeji_Liaochengsum +=  (entity.KakouYeji_Liaocheng + entity.KakouYeji_Chanpin + entity.KakouYeji_TeshuChanpin + entity.KakouYeji_Xiangmu + entity.KakouYeji_Daxiangmu + entity.KakouYeji_Teshuxiangmu);
        
_Ticheng +=   entity.Ticheng;
    <tr>
        <td style="border-right: 1px solid #f1eaea;">
            @entity.UserID
        </td>
        <td style="border-right: 1px solid #f1eaea;">
            @entity.UserName
