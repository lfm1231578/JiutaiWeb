@{
    
}
<div id="mainContentBody">
    @Html.Partial("_ReservationControl")
    <div id="mainContentRight">
        <form action="" method="post" class="clearfix" name="myform" id="myform">
        
        <div class="mainContentRight_top">
            <label class="Soliddots">
              </label>
            预约排班设置</div>
        <div class="mainContentRight_bottom" id="mainContentRight_bottomInfo" style="position: relative;">
            <div class="mainContentRight_bottomContent">
                <div id="tabbox">
                    <ul class="tabs" id="tabs">
                        <li><a href="###" tab="tab1">排班设置</a></li>
                        <li><a href="###" tab="tab2">排班列表</a></li>
                        
                    </ul>
                    <ul id="tab_conbox" class="tab_conbox">
                        <li id="tab1" class="tab_con" style="height: 100%; padding:0px;">
                            <iframe src="@Url.Content("~/ReservationDoctorManage/PaibanSet?")" href="@Url.Content("~/ReservationDoctorManage/PaibanSet")" frameborder="0" style="width:100%;_border:none;border:none;   border:0px; _border:0px;  ">
                            </iframe>
                        </li>
                        <li id="tab2" class="tab_con" style="display: none; padding:0px;">
                            <iframe id="PatientDetailPage" src="" href="@Url.Content("~/ReservationDoctorManage/PaibanList")" frameborder="0" style="width:100%;_border:none;border:none;   border:0px; _border:0px;  ">
                            </iframe>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        </form>
    </div>
</div>
<script type="text/javascript">
    jQuery(document).ready(function () {
        $("iframe").height(getDocumentHeight() - $("#header").height() - $("#footer").height() - 110);
        //Tab切换
        $("#tabs  li").click(function () {
            $("#tabs li").removeClass("thistab");
            $(this).addClass("thistab");
            $("#tabs li").children("a").css("color", "#666", "textShadow", "1px 1px 1px #868686");
            $(this).children("a").css("color", "lightsalmon", "textShadow", "1px 1px 1px #868686");

            $(".tab_con").hide();
            var index = $(this).parent().find("li").index(this);
            $("#tab_conbox").find(".tab_con").eq(index).show();
            var iframe1 = $("#tab_conbox").find(".tab_con").eq(index).find("iframe");
            iframe1.attr("src", iframe1.attr("href"));
        });
        $("#tabs li:first").addClass("thistab").show();
        $("#tabs li:first").children("a").css("color", "lightsalmon");
    });
</script>
