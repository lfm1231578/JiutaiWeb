            error: function (request) {
                alert("保存出错");
            },
            success: function (data) {
                if (data > 0) {
                    artdialog("保存成功！");
                    if (uid > 0) {
                        history.go(-1);
                    } else {
                        artdialog("保存成功！", "@Url.Content("~/PatientManage/PatientInfoManage")");
