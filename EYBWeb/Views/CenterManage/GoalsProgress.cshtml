</style>
<div class="mainContentRight_bottom toolbar" style="overflow: hidden;">
    <div class="accountdiv l-list ullist" style="margin: 0px; width: 100%;">
        <ul id="tdul">
            <li>
                <input type="button" class="submitbutton3" style="" onclick="history.go(-1)" value="返回" />
            </li>
            <li>
                <input type="button" class="submitbutton3" style="" onclick="Preview1()" value="打印" />
            </li>
        </ul>
        <div style="clear: both;">
        </div>
    </div>
    <div class="box-wrap" id="form1" style="width: 99%; margin-left: 4px;">
        <table class="GoalTab" style="border: 1px solid rgb(0, 176, 222); border-collapse: collapse;">
            <thead>
                <tr style="line-height: 45px;">
                    <th colspan="10" style="height: 45px; font-size: 26px; border-bottom: 1px solid #ccc;">@Model.Name
                    </th>
                </tr>
                <tr  style="line-height: 30px; height: 30px;">
                    <td  style="width: 60px;">
                    </td>
                    <td colspan="5">
                    </td>
                    <td style="font-size: 16px; font-weight: bolder; text-align: right;">
                        员工姓名:
                    </td>
                    <td style="font-size: 16px; font-weight: bolder; text-align: left;">
                        @Model.UserName
                    </td>
                    <td style="font-size: 16px; font-weight: bolder; text-align: right;">
                        目标时间
                    </td>
                    <td style="font-size: 16px; font-weight: bolder; text-align: left;">
                        @(Model.Years + "年" + Model.Months + "月")
                    </td>
                </tr>
            </thead>
            <tbody style="border: 1px solid rgb(0, 176, 222); border-collapse: collapse;">
                <tr style="line-height: 26px; background: rgb(0, 176, 222);">
                    <th style="line-height: 26px; border-collapse: collapse; border: 1px White solid;
                        text-align: center; color: White; font-size: 14px;">
                        日期
                    </th>
                    <th style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;" colspan="4">
                        现金
                    </th>
                    <th style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;" colspan="4">
                        实操
                    </th>
                    <th style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;" colspan="4">
                        客流量
                    </th>
                </tr>
                <tr style="background: rgb(0, 176, 222); border-collapse: collapse;">
                    <th style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                    </th>
                    <td title="现金月目标" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        月目标
                    </td>
                    <td colspan="3" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        @Model.Xianjin
                    </td>
                    <td title="实操月目标" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        月目标
                    </td>
                    <td colspan="3" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        @Model.ShiHao
                    </td>
                    <td title="客流量目标" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        月目标
                    </td>
                    <td colspan="3" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        @Model.Traffic
                    </td>
                </tr>
                <tr style="background: rgb(0, 176, 222); border-collapse: collapse;">
                    <td style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                    </td>
                    <td title="现金" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        日完成
                    </td>
                    <td title="现金" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        累计完成
                    </td>
                    <td title="现金" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        完成率
                    </td>
                    <td title="累计差异" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        累计差异
                    </td>
                    <td title="实操" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        日完成
                    </td>
                    <td title="实操" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        累计完成
                    </td>
                    <td title="实操" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        完成率
                    </td>
                 <td title="累计差异" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        累计差异
                    </td>
                    <td title="客流量" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        日完成
                    </td>
                    <td title="客流量" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        累计完成
                    </td>
                    <td title="客流量" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        完成率
                    </td>
                  <td title="累计差异" style="line-height: 26px; text-align: center; color: White; border: 1px White solid;
                        font-size: 14px;">
                        累计差异
                    </td>
                </tr>
                @Html.Raw(Model.StrHtml)
            </tbody>
        </table>
        <div style="clear: both;">
        </div>
        <object id="LODOP_OB" classid="clsid:2105C259-1E0C-4534-8141-A753534CB4CA" width="0"
            height="0">
            <embed id="LODOP_EM" type="application/x-print-lodop" width="0" height="0"></embed>
        </object>
    </div>
    <div style="width: 100%; height: 10px;">
    </div>
    <div style="clear: both;">
    </div>
</div>
<script type="text/javascript">
    var LODOP; //声明为全局变量 
    function Preview1() {
        LODOP = getLodop(document.getElementById('LODOP_OB'), document.getElementById('LODOP_EM'));
        // LODOP.PRINT_INITA(0, 0, 580, 333, "自定义纸张1"); LODOP.PRINT_INIT("自定义纸张"); // document.getElementById('W1').value;
        LODOP.SET_PRINT_PAGESIZE(1, 0, 0, "A4");
        LODOP.SET_PRINTER_INDEXA(-1); //指定打印机
        LODOP.SET_PRINT_MODE("RESELECT_PAGESIZE", true);
        AddPrintContent();
          LODOP.PREVIEW();
    };
    function AddPrintContent() {
        // LODOP.SET_PRINT_STYLE("FontColor", 16711680);
        //LODOP.ADD_PRINT_RECT(62, 16, 459, 217, 0, 1);
        //LODOP.SET_PRINT_STYLEA(2, "FontSize",16);
        LODOP.SET_PRINT_STYLEA( "FontName", "宋体");
        LODOP.SET_PRINT_STYLE("FontSize", 10);
        LODOP.SET_PRINT_STYLE( "FontColor", 0);
        var form1height = $("#form1").height();
        LODOP.ADD_PRINT_HTM(20, 90, "100%", "100%", document.getElementById("form1").innerHTML);
    } 
</script>
