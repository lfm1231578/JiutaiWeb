@{
    var UserID = Request["UserID"];
}
<div id="mainContentBody">
    @Html.Partial("_CashControl")
    <div id="mainContentRight">
        <form action="" method="post" class="clearfix" name="myform" id="myform">
        <input type="hidden" name="UserID" id="UserID" value="@UserID"/><!-- 顾客ID-->
        <div class="mainContentRight_top">
            <label class="Soliddots">
               </label>
            寄存/欠货 管理</div>
        <div class="mainContentRight_bottom" id="mainContentRight_bottomInfo" style="position: relative;">
            <div class="mainContentRight_bottomContent">
                <div id="tabbox">
                    <ul class="tabs" id="tabs">
                        <li><a href="###" tab="tab1">寄存</a></li>
                        <li><a href="###" tab="tab2">欠货</a></li>
                    </ul>
                    <ul id="tab_conbox" class="tab_conbox">
                        <li id="tab1" class="tab_con" style="height: 100%; width: 100%;padding:0px;">
                            <iframe id="iframeJiChu" src="@Url.Content("~/CashManage/ProductJichunIn")" href="@Url.Content("~/CashManage/ProductJichunIn")" frameborder="0"  style="_border:none;border:none; height:100%; width:100%; margin:0px; padding:0px;  border:0px; _border:0px;  ">
                            </iframe>
                        </li>
                        <li id="tab2" class="tab_con" style="height: 100%; width: 100%;display: none; padding:0px;">
                            <iframe src="@Url.Content("~/CashManage/ProductJichunOut")" href="@Url.Content("~/CashManage/ProductJichunOut")" frameborder="0"  style="_border:none;border:none; height:100%; width:100%; margin:0px; padding:0px;  border:0px; _border:0px;  ">
                            </iframe>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        </form>
    </div>
</div>
<script type="text/javascript">
    jQuery(document).ready(function () {
        $("iframe").height(getDocumentHeight() - $("#header").height() - $("#footer").height() - 110);
        //Tab切换
        $("#tabs  li").click(function () {
            $("#tabs li").removeClass("thistab");
            $(this).addClass("thistab");
            $("#tabs li").children("a").css("color","#666","textShadow", "1px 1px 1px #868686")
            $(this).children("a").css("color","lightsalmon","textShadow", "1px 1px 1px #868686")
            $(".tab_con").hide();
            var index = $(this).parent().find("li").index(this);
            $("#tab_conbox").find(".tab_con").eq(index).show();
            var iframe1 = $("#tab_conbox").find(".tab_con").eq(index).find("iframe");
            iframe1.attr("src", iframe1.attr("href"));
        });
        $("#tabs li:first").addClass("thistab").show();
        $("#tabs li:first").children("a").css("color", "lightsalmon");
    });
</script>
