        var PrintXuanNameEntity = systemBLL.GetUserSettingsValue(ViewBag.HospitalID, "PrintXuanName");

        //打印机设置
        var PrintTemplate = systemBLL.GetUserSettingsValue(ViewBag.HospitalID, "PrintTemplate").AttributeValue;

