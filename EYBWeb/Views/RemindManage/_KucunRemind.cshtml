@using Webdiyer.WebControls.Mvc;
@model PagedList<EYB.ServiceEntity.WarehouseEntity.ProductStockEntity>
@{
    Layout = null;

}
<table class="gridTable" cellpadding="0" cellspacing="0">
    <thead>
        <tr style="height: 30px;">
            <th>
                门店仓库
            </th>
            <th>
                产品名称
            </th>
            <th>
                产品分类
            </th>
            <th>
                品牌
            </th>
            <th>
                库存数量
            </th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var info in Model)
        {
            <tr>
                <td>
                    @info.HouseName
                </td>
                <td>
                    @info.ProductName
                </td>
                <td>
                    @info.TypeName
                </td>
                <td>
                    @info.BrandName
                </td>
                <td>
                    @info.Quatity
                </td>
                
            </tr>
        }
        @Html.Raw(HS.SupportComponents.NoRecord.NoRecords(Model.Count, 5))
    </tbody>
</table>
@Ajax.Pager(Model, new PagerOptions { PageIndexParameterName = "pageNum", PagerItemsSeperator = "", ShowPageIndexBox = false, CurrentPagerItemWrapperFormatString = "<span class=\"current\">{0}</span>" }, new MvcAjaxOptions { UpdateTargetId = "articles", HttpMethod = "Post", DataFormId = "searchForm" }, new { id = "badoopager" })</form>
<script>
    $(function () {
        $("#sp").text('@Model.TotalItemCount');
    })
</script>