@using SystemManage.Factory.IBLL;
@using HS.SupportComponents;
@{
    Layout = "~/Views/Shared/Header.cshtml";
    var systemBLL = HS.SupportComponents.Base.XmlRegistration.Current.GetResovleContainer().Resolve<ISystemManageBLL>();
}



<form name="searchform" action="" method="get">
<table width="100%" cellspacing="0" class="search-form">
    <tbody>
        <tr>
            <td>
                <div class="explain-col">
                    开始时间：
                    <input class="Wdate" id="StartTime" type="text"  name="DutyTime" value="@DateTime.Now.AddDays(-7)" onClick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss'})" />
                   
                    -结束时间
                     <input class="Wdate" id="EndTime" type="text"  name="DutyTime" value="@DateTime.Now" onClick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss'})" />
                    
                    &nbsp;操作模块：
                    <input type="text" id="ModuleList" value="" />
                    &nbsp;账号 :
                    <input type="text" id="txtUserAllInfo" value="" />
                    <input type="button" id="btnsearch" name="search" class="button" value="搜索" />
                </div>
            </td>
        </tr>
    </tbody>
</table>
</form>

<div class="pageContent">
    <div class="panelTable">
        <table id="logList">
        </table>
        <div id="logListPage">
        </div>
    </div>
</div>

<script type="text/javascript" src="@Url.Content("~/Scripts/Controls/SystemManage/LogManage.js")"></script>
