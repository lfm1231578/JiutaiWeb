<style type="text/css">
    .title { line-height: 40px; text-align: center; font-size:20px;}
    .ltitle { display: inline-block; float: right; text-align: center; margin-right: 10px; }
    .ltitle label { font-style: inherit; color: Red; }
    table { border-collapse: collapse; border: none; width: 100%;margin-left:10px; background: #eee; }
