@{
    ViewBag.Title = "addWarehouse";
    Layout = null;
}
<link href="@Url.Content("~/Content/themes/css/core.css")?v=1.1" rel="stylesheet" type="text/css" media="screen" />
<link href="../../Content/themes/css/template.css" rel="stylesheet" type="text/css" />
<script src="../../Scripts/Jquery/jquery-1.8.2.min.js" type="text/javascript"></script>
<script src="../../Scripts/Jquery/plugins/artDialog/artDialog.js" type="text/javascript"></script>
<script src="../../Scripts/Jquery/plugins/artDialog/plugins/iframeTools.js" type="text/javascript"></script>
<body style="background: #fff;">
<div style=" width:100%; height:26px;"></div>
    <lable style=" margin-left:25px;">仓库名称:</lable> <input type="text" name="warename" style=" width:130px;height:26px;" />
 <div style="margin: 15px 20px 0 0; margin-left: 0px;" class="clearfix">
            <input type="button" class="submitbutton2" value="保 存" id="btn" style=" margin-left:55px;" />
      @*      <input class="submitbutton2" type="button" onclick="closeedNorefresh()" value="取 消" />*@</div>
</body>
 <script type="text/javascript">
     $("#btn").click(function () {
         var value = $("input[name='warename']").val();
         if (value == '' || value == null) {
             alert("请填写仓库名称!");
             return false;
         }
         art.dialog.data('WValue', value); // 存储数据
         art.dialog.close();
     })

     function closeedNorefresh() {
         art.dialog.data('WValue', "");
         art.dialog.close();
     }
 </script>

