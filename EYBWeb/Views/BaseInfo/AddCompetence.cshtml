@{
    ViewBag.Title = "AddCompetence";
    Layout = null;
}
<link href="@Url.Content("~/Content/themes/css/core.css")?v=1.1" rel="stylesheet" type="text/css" media="screen" />
<link href="../../Content/themes/css/template.css" rel="stylesheet" type="text/css" />
<script src="../../Scripts/Jquery/jquery-1.8.2.min.js" type="text/javascript"></script>
<script src="../../Scripts/Common/common.js" type="text/javascript"></script>
<script src="../../Scripts/Jquery/plugins/artDialog/artDialog.js" type="text/javascript"></script>
<script src="../../Scripts/Jquery/plugins/artDialog/plugins/iframeTools.js" type="text/javascript"></script>
<script src="../../Scripts/Jquery/plugins/artDialog/artDialog.js?skin=blue" type="text/javascript"></script>
<script src="../../Scripts/Jquery/plugins/artDialog/plugins/iframeTools.js" type="text/javascript"></script>
<script src="../../Scripts/Common/wbox-min.js" type="text/javascript"></script>
<body style="background: #fff;">
    <div style="height: 35px; width: 100%;">
    </div>
    <form id="myform2">
    <label style="margin-left: 20px;">
        权限名称:</label>
    <input type="text" class="VersionName" name="VersionName" style="width: 160px; height: 26px;
        line-height: 26px;" />
    <input type="button" class="submitbutton2" value="保 存" onclick="FullValue()" style="float: none;
        display: block; text-align: center; margin-left: 100px; margin-top: 20px;" />
    </form>
</body>
<script type="text/javascript">
    function FullValue() {
        var val = $(".VersionName").val();
        if (val == '') {
            artdialogfailTwo("请填写权限名称！", "", "250", "100");
            return false;
        }

        $.ajax({
            type: "POST",
            url: getControllerUrl("BaseInfo", "CompetenceAdd"),
            async: false,
            data: $('#myform2').serialize(), // 你的formid
            error: function (request) {
                artdialogfailTwo("操作失败！", "", "250", "100");
            },
            success: function (data) {

                if (data != null) {
                    artdialogfail("操作成功！", "", "250", "100");
                    closeed();
                } else {
                    artdialogfailTwo("操作失败,！", "", "250", "100");
                }
            }
        });
    }
</script>
